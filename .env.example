# Custom dbt MCP Server Configuration

# Required: Path to your dbt project directory
DBT_PATH=/path/to/your/dbt/executable
DBT_PROJECT_DIR=/path/to/your/dbt/project

# Optional: Path to dbt profiles directory (defaults to ~/.dbt)
# DBT_PROFILES_DIR=/path/to/dbt/profiles

# Tool filtering (already set in server.py but can be overridden)
DISABLE_SEMANTIC_LAYER=true
DISABLE_DISCOVERY=true
DISABLE_REMOTE=true